            android:layout_height="wrap_content"
            android:background="@color/black"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/list_view_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:orientation="vertical">


